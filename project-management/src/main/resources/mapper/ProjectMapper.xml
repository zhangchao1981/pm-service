<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.iscas.pm.api.mapper.ProjectMapper">

    <resultMap id="BaseResultMap" type="com.iscas.pm.api.model.project.Project">
            <result property="id" column="id" jdbcType="VARCHAR"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
    </resultMap>

    <insert id="addProject" parameterType="com.iscas.pm.api.model.project.Project" keyProperty="id">
        insert into project
        (
        id,name,secretLevel
        )
        values
        (
        #{id,jdbcType=VARCHAR},
        #{name,jdbcType=VARCHAR},
        #{secretLevel,jdbcType=VARCHAR}
        )
    </insert>

    <select id="selectAllById" resultMap="BaseResultMap">
        select  * from project
    </select>
</mapper>
