<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="test.mapper.EnvInformationMapper">

    <resultMap id="BaseResultMap" type="test.EnvInformation">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="category" column="category" jdbcType="VARCHAR"/>
            <result property="serverAddress" column="server_address" jdbcType="VARCHAR"/>
            <result property="username" column="username" jdbcType="VARCHAR"/>
            <result property="password" column="password" jdbcType="VARCHAR"/>
            <result property="explain" column="explain" jdbcType="VARCHAR"/>
            <result property="planId" column="plan_id" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,category,server_address,
        username,password,explain,
        plan_id
    </sql>
</mapper>
